{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "albertcht/invisible-recaptcha": "^1.9",
        "arcanedev/log-viewer": "~5",
        "artesaos/seotools": "^0.16.0",
        "awssat/laravel-visits": "^2.2",
        "barryvdh/laravel-cors": "^0.11.4",
        "beyondcode/laravel-websockets": "^1.3",
        "botman/botman": "^2.5",
        "botman/driver-web": "^1.5",
        "cartalyst/stripe-laravel": "11.0.*",
        "classiebit/addchat-laravel": "^1.0",
        "codebyray/laravel-review-rateable": "^1.2",
        "consoletvs/charts": "6.*",
        "fideloper/proxy": "^4.0",
        "genealabs/laravel-model-caching": "*",
        "intervention/image": "^2.5",
        "joedixon/laravel-translation": "^1.0",
        "laracasts/utilities": "^3.0",
        "laravel-frontend-presets/material-dashboard": "^1.0",
        "laravel/framework": "^6.2",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^1.0",
        "laraveldaily/laravel-invoices": "^1.3",
        "livewire/livewire": "^0.5",
        "mariuzzo/laravel-js-localization": "^1.5",
        "mcamara/laravel-localization": "^1.4",
        "nyholm/psr7": "^1.2",
        "php-http/curl-client": "^2.0",
        "php-http/message": "^1.8",
        "proengsoft/laravel-jsvalidation": ">2.5.0",
        "pusher/pusher-php-server": "^4.1",
        "realrashid/sweet-alert": "^2.0",
        "santigarcor/laratrust": "5.2.*",
        "silber/page-cache": "^1.0",
        "spatie/laravel-searchable": "^1.5",
        "spatie/laravel-tags": "^2.5",
        "spatie/laravel-translatable": "^4.2",
        "srmklive/paypal": "^1.6",
        "torann/currency": "^1.0",
        "torann/geoip": "^1.0",
        "treestoneit/shopping-cart": "^1.1",
        "tymon/jwt-auth": "1.0.0-rc.5",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.0",
        "yajra/laravel-datatables-oracle": "^9.0",
        "zgabievi/promocodes": "^2.3"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.1",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Http/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
